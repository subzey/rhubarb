{
  "name": "rhubarb",
  "version": "0.1.0-rc2",
  "description": "Compile-time js constant inliner",
  "main": "lib/rhubarb.node.js",
  "author": "Anton Khlynovskiy <subzey@gmail.com>",
  "license": "WTFPL",
  "dependencies": {
    "esprima": "^2.0.0"
  },
  "devDependencies": {
    "jshint": "^2.6.3",
    "mocha": "^2.2.1"
  },
  "scripts": {
    "test-mocha": "node ./node_modules/mocha/bin/mocha -R dot",
    "test-jshint": "node ./node_modules/jshint/bin/jshint lib",
    "test": "npm run test-mocha & npm run test-jshint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subzey/rhubarb.git"
  },
  "keywords": [
    "inline",
    "constants",
    "build",
    "compile"
  ],
  "directories": {
    "lib": "./lib"
  },
  "jshintConfig": {
    "eqnull": true,
    "strict": true,
    "curly": true,
    "noarg": true,
    "unused": true,
    "undef": true,
    "-W004": true,
    "node": true
  }
}
